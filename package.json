{
  "name": "dassh",
  "version": "1.0.0",
  "description": "Constitutional widget-based dashboard application",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:e2e": "turbo run test:e2e",
    "test:a11y": "turbo run test:a11y",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:generate": "turbo run db:generate --filter=@dassh/shared",
    "db:migrate": "turbo run db:migrate --filter=@dassh/shared",
    "db:studio": "turbo run db:studio --filter=@dassh/shared"
  },
  "keywords": [
    "dashboard",
    "widgets",
    "constitutional",
    "nextjs",
    "monorepo"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@jest/globals": "^30.1.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.36.0",
    "jest": "^30.1.3",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  }
}
