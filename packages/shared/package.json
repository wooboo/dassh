{
  "name": "@dassh/shared",
  "version": "0.1.0",
  "private": true,
  "description": "Shared utilities, types, and services for dassh project",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./database": {
      "types": "./src/database/index.ts",
      "default": "./src/database/index.ts"
    },
    "./auth": {
      "types": "./src/auth/index.ts",
      "default": "./src/auth/index.ts"
    },
    "./websocket": {
      "types": "./src/websocket/index.ts",
      "default": "./src/websocket/index.ts"
    },
    "./validation": {
      "types": "./src/validation/index.ts",
      "default": "./src/validation/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .turbo node_modules",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@kinde-oss/kinde-auth-nextjs": "^2.9.2",
    "drizzle-orm": "^0.44.5",
    "postgres": "^3.4.7",
    "ws": "^8.18.3",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.18.1",
    "drizzle-kit": "^0.31.4",
    "typescript": "^5.9.2"
  }
}